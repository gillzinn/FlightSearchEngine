                      minWidth="-Infinity" prefHeight="40.0" prefWidth="350.0">
                    <children>
                        <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                               prefHeight="40.0" prefWidth="100.0" text="Name:" HBox.hgrow="NEVER">
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="fxName" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
